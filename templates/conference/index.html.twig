{% extends 'base.html.twig' %}

{% block title %}Conference Guestbook!{% endblock %}

{% block body %}
    <h2>Give your feedback!</h2>

    {% for conference in conferences %}
        <p>
        <h4><a href="{{ path('conference', { id: conference.id }) }}">{{ conference }}</a></h4>
        </p>
    {% endfor %}
{% endblock %}
